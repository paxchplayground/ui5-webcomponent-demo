<mvc:View controllerName="demo.ui5-webcomponent-demo.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Panel>
							<html:ui5-badge color-scheme="8">
								<html:ui5-icon name="lab" slot="icon"></html:ui5-icon>
							</html:ui5-badge>
							<html:ui5-badge color-scheme="1">
								<html:ui5-icon name="accept" slot="icon"></html:ui5-icon>done
							</html:ui5-badge>
						</Panel>
						<Panel>
							<html:ui5-datepicker></html:ui5-datepicker>
						</Panel>
						<Panel>
							<html:ui5-datepicker theme='sap_belize_hcb' primary-calendar-type='Islamic'></html:ui5-datepicker>
						</Panel>
						<Panel>
							<html:game-of-life size="20"></html:game-of-life>
						</Panel>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>